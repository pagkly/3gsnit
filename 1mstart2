wa="45kFvJmKx6DapX1bQ35YzLVAs8fa41XnAMZT952QCRRrC95DFCqnFC8NC4WfcVwKDTY8qJiFkYQu9F7E4rLcyVK8P8WQtxo"
userno="x"
xmrs="xmr-stak/build/bin/xmr-stak"
xmrig="xmrig/build/xmrig"
xmrign="xmrig-nvidia/build/xmrig-nvidia"
