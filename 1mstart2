startgnirehtet() {
startandtdir="$dir/androidtether/gnirehtet-java/gnirehtet" &&\
sudo chmod +x $startandtdir &&\
xterm -hold -e "$startandtdir run" &
}
runvpn() {
xterm -hold -e \"$dir/FOVPN/FreeVPNconnect.sh\"
}
startworking() {
if [[ -z $(awk '/nautilus|gedit|chrome/' <<< "$(ps cax)" ) ]] ; then
#https://stackoverflow.com/questions/9117507/linux-unix-command-to-determine-if-process-is-running
nautilus "$userhomedir/Documents/Documentsv2" & disown
gedit "$gsnitdir" & disown
checkxmr & disown
fi
}

#####FNENGINE
traprestart() {
trap SIGINT
echo $DIR0
#xterm -hold -e "$DIR0 1" & disown $!
gnome-terminal -e "$DIR0 1" & disown $!
sleep 1
exit
return 0
}
populatefn() {
if [[ -n "$3" ]] ; then
startno="$3"
else
startno="0"
fi
allfn=$(awk '/(^'$1'\(\) {)/ {gsub("([(){])","");print}' $2)
popfn="${popfn}"$'\n'"$allfn"
allfnarray=($allfn)
popfnarray=($popfn)
totalallfn=$[${#allfnarray[@]}+$startno]
echo "$totalallfn"
}
printallfn() {
for f in ${!popfnarray[@]} ; do
echo $[$f+1]".${popfnarray[f]}"
done
}
errorc=0
fn1="choosetheme|staticnomanual|startgnirehtet|resetdesktop|ipmode|checkxmr|gitpushgsm|startworking"
choosefn() {
read fnno
n=$[$fnno-1]
fn="${popfnarray[$[$fnno-1]]}"
echo "$n.$fn"
if [[ $fn =~ (wintheme) ]] || [[ $fnno =~ (wintheme) ]] ; then
$fn
elif [[ $fn =~ (inetstaton) ]] || [[ $fnno =~ (inetstaton) ]]; then
read -p 'COMPNO : ' number
echo "Static ip for $number!"
$fn "$number" &
elif [[ $fn =~ (loopstaticip) ]] || [[ $fnno =~ (loopstaticip) ]] ; then
$fn &
elif [[ $fn =~ (minertype) ]] || [[ $fnno =~ (minertype) ]] ; then
read -p "MTYPE :"  mtype
echo "$mtype!"
$fn "$mtype"
elif [[ $fn =~ ($fn1) ]] ; then
$fn &
elif [[ $fnno =~ ($fn1) ]] ; then
$fnno &
elif [[ $fnno =~ (ip|ipmode) ]] ; then
ipmode
else
if [[ $errorc -eq 100 ]] ; then
sleep 1440000
fi
echo "nftrya"
((errorc++))
fi
return 0
}
runlistfn() {
while true ; do
popfn=""
trap "traprestart" INT
populatefn "(wintheme|inetstaton|checkdhcp|minertype|runvpn|traprestart|$fn1)" "$DIR0"
printallfn
choosefn
trap SIGINT
done
}
#sfiledir=/cdrom/start
#sfiledir=$mdir/MB6/start
detectrunpreenvo() {
texttodetect="(runpreenv)"
sfiledir2=/cdrom/start
dtextobject=$(detext "$sfiledir2" "$texttodetect")
if [[ ! -z $dtextobject ]] ; then
runpreenv="$dtextobject"
echo "rpenvo=$runpreenv"
fi
}
#detectrunpreenvo
prerun() {
runsetvar
runpreenv=$(detectrunpreenvo)
if [[ ! -f $dir/setupdone ]] ; then
touch $dir/setupdone ;
setupdocdir
setupdocdir2
#recheck
cpgsnit
fi
wintheme &
checkdhcp &

if [[ -f "$USBMBIOSdir" ]] ; then
startworking
fi
##sudo cp $mdir/MB4/Pass/* /etc/NetworkManager/system-connections
}
xhost +
prerun
if [[ $1 =~ ^(1)$ ]] ; then
runlistfn
#else
#sudo service lightdm stop
fi
